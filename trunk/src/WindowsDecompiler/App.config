      </Environment>
    </Environments>
    <Architectures>
      <Architecture Name="x86-real-16" Description="Intel x86 16-bit Real Mode" Type="Decompiler.Arch.Intel.IntelProcessorArchitecture,Decompiler.Arch.Intel.dll"/>
