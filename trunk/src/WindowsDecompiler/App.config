    <Environments>
      <Environment Name="win32" Description="Win32" Type="Decompiler.Environment.Win32.Win32Platform,Decompiler.Environment.Win32">
        <Architecture Name="x86-protected-32"/>
      </Environment>
      <Environment Name="ms-dos" Description="MS-DOS" Type="Decompiler.Environment.MsDos.MsdosPlatform,Decompiler.Environment.MsDos">
