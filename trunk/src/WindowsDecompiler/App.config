<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="Decompiler">
      <section name="Loaders" type="Decompiler.Configuration.LoaderSectionHandler,Decompiler" />
      <section name="Architectures" type="Decompiler.Configuration.ArchitectureSectionHandler,Decompiler"/>
      <section name="Environments" type="Decompiler.Configuration.OperatingEnvironmentSectionHandler,Decompiler"/>
